{
    "version": 2,
    "builds": [
      {
        "src": "src/app/api/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "src/app/page.jsx",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/app/api/$1",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  